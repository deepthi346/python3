if __name__ == '__main__':
    a = int(input())
    b = int(input())
    division = a//b;
    print(division)
    division = a/b;
    print(division)

if __name__ == '__main__':
    a = int(input())
    b = int(input())
    sum = a + b;
    print(sum)
    difference = a - b;
    print(difference)
    product = a * b;
    print(product)
