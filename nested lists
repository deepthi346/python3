n = int(input())
students = []

for _ in range(n):
    name = input()
    grade = float(input())
    students.append([name, grade])

# Find the second lowest grade
unique_grades = sorted(set([student[1] for student in students]))
second_lowest = unique_grades[1]

# Get names of students with second lowest grade
result = sorted([student[0] for student in students if student[1] == second_lowest])

# Print names
for name in result:
    print(name)
